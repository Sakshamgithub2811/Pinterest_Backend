<%- include('../partials/header') %>

<div class="w-full min-h-screen bg-zinc-800 px-10 py-8">
    <div class="cards flex flex-wrap gap-10">
        <% posts.forEach(function(post){ %>
        <div class="card w-32" >
            <div class="w-full h-40 bg-sky-200 rounded-lg">
                <img class="w-full  h-full object-cover" src="/upload/<%= post.image%>" alt="">
            </div>
            <h5 class="mt-2 text-sm"><%= post.title %></h5>
        </div>
        <% }) %>
    </div>
</div>
<%- include('../partials/footer') %>